{
	"comment": "Module definition file.",
	"name": "jobs",
	"description": "View and post OCDLA jobs.",
	"files": [
		"Job.php",
		"ListTemplate.php",
		"/models/salesforce/Job__c.php"
	],
	"routes": {
		"jobs": {
			"callback":"home",
			"content-type":"text\/html"
		},
		"jobs/new": {
			"callback": "postingForm",
			"content-type": "text\/html"
		},
		"jobs/create": {
			"callback": "createPosting",
			"content-type": "text\/html"
		},
		"job/edit/%id":{
			"callback": "edit",
			"content-type": "text\/html"
		},
		"job/delete/%sobjectType/%id":{
			"callback": "delete",
			"content-type": "text\/html"
		}
	}
}